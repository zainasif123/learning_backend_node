<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ID List</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
        }

        h1 {
            text-align: center;
        }

        ul {
            list-style-type: none;
            padding: 0;
        }

        li {
            padding: 8px;
            margin: 5px 0;
            background-color: #f0f0f0;
            border: 1px solid #ddd;
            border-radius: 5px;
        }
    </style>
</head>

<body>
    <h1>List of IDs</h1>
    <% if (urls && urls.length> 0) { %>
        <ul>
            <% urls.forEach(function(id) { %>
                <li>
                    <%= id.shortUrl %>
                </li>
                <% }) %>
        </ul>
        <% } else { %>
            <p>No IDs available.</p>
            <% } %>
</body>

</html>